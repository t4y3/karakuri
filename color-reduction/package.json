{
  "name": "color-reduction-algorithm",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:js & npm run build:css",
    "build:js": "rollup -c",
    "build:css": "NODE_ENV=production postcss public/global.css -o public/index.css",
    "dev": "npm run dev:js & npm run dev:css",
    "dev:js": "rollup -c -w",
    "dev:css": "postcss public/global.css -o public/index.css -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "deploy": "rm -rf ../docs/color-reduction && cp -r public ../docs/color-reduction"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tailwindcss/ui": "^0.7.2",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^10.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "mediancut": "^2.1.0",
    "sirv-cli": "^1.0.0",
    "taxis": "0.0.1",
    "tweakpane": "^1.5.8"
  }
}
